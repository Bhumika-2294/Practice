// Write code of  Perfect number

#include <iostream>
using namespace std;
 int is_perfect(int n){
     int sum=1 ;
     for (int i=2;i<=n/2 ;i++){
         if (n%i==0){
             sum=sum+i;
         }
     }
     if(sum==n){
         cout<<"yes";
     }
     else{
         cout<<"No";
     }
   return 0;  
 }

int main() {
    int x;
    cin>>x;
    is_perfect(x);

    return 0;
}
